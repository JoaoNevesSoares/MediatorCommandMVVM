<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml">
    <GridPane hgap="10" vgap="5">
        <Label text="Insert Process Pid" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Label text="Burst time" fx:id="burstLabel" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Spinner fx:id="pidSpinner" min="0" max="999" initialValue="0" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Spinner fx:id="burstSpinner" min="0" max="999" initialValue="0" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</VBox>
